{
  "name": "click-emulation",
  "version": "0.1.2",
  "author": "Yomotsu",
  "license": "MIT",
  "main": "dist/click-emulation.js",
  "jsnext:main": "dist/click-emulation.module.js",
  "module": "dist/click-emulation.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "yomotsu/click-emulation",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-mdcs": "^5.0.0",
    "rollup": "^3.24.0",
    "terser": "^5.17.7",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config && terser dist/click-emulation.js -o dist/click-emulation.min.js --comments '/^!/'",
    "lint": "eslint src --ext ts"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not dead"
  ],
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": "mdcs",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": 1,
      "indent": 0,
      "@typescript-eslint/indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1,
          "flatTernaryExpressions": true
        }
      ],
      "no-multi-spaces": [
        0
      ],
      "no-trailing-spaces": [
        "error",
        {
          "ignoreComments": true
        }
      ],
      "key-spacing": [
        0
      ]
    }
  },
  "eslintIgnore": [
    "/dist/",
    "/node_modules/"
  ],
  "typesVersions": {
    "<=3.4.0-0": {
      "*": [
        "./"
      ]
    }
  },
  "keywords": []
}
